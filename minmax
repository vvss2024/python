test_list = [8, 1, 7, 10, 5]
min_ele, max_ele = test_list[0], test_list[0]

for i in range(1, len(test_list)):

	if test_list[i] < min_ele:
		min_ele = test_list[i]
		
	if test_list[i] > max_ele:
		max_ele = test_list[i]
		
print('Minimum Element in the list', test_list, 'is', min_ele)

print('Maximum Element in the list', test_list, 'is', max_ele)
